<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uas_android"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MountainInfoActivity"
            android:label="@string/title_activity_mountain_info" >
        </activity>
        <activity
            android:name=".MountainInfoDetailActivity"
            android:label="@string/title_activity_mountain_info_detail" >
        </activity>
        <activity
            android:name=".MountainMap"
            android:label="@string/title_activity_mountain_map" >
        </activity>
        <activity
            android:name=".MountainMapActivity"
            android:label="@string/title_activity_mountain_map" >
        </activity>
        <activity
            android:name=".MountainMapDetailActivity"
            android:label="@string/title_activity_mountain_map_detail" >
        </activity>
        <activity
            android:name=".EquipmentActivity"
            android:label="@string/title_activity_equipment" >
        </activity>
        <activity
            android:name=".PrepareActivity"
            android:label="@string/title_activity_prepare" >
        </activity>
    </application>

</manifest>
